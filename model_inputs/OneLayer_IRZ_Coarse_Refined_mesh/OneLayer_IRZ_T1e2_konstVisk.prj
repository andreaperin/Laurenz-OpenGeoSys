    <time_loop>
        <processes>
            <process ref="thermohydraulic">
                <nonlinear_solver>basic_picard</nonlinear_solver>
                <convergence_criterion>
                    <type>PerComponentDeltaX</type>
                    <norm_type>NORM2</norm_type>
					<reltols>1e-5 1e-5</reltols>
					<abstols>1e-8 1e-8</abstols>
                </convergence_criterion>
                <time_discretization>
                    <type>BackwardEuler</type>
                </time_discretization>
				<time_stepping>
					<type>FixedTimeStepping</type>
					<t_initial>0.0</t_initial>
					<t_end>1576800000</t_end>
					<!--31536000 1Jahr-->
					<timesteps>
						<pair>
							<repeat>1</repeat>
							<delta_t>10</delta_t>
						</pair>						
						<pair>
							<repeat>20</repeat>
							<delta_t>360</delta_t>
						</pair>
						<pair>
							<repeat>35</repeat>
							<delta_t>2220</delta_t>
						</pair>
						<pair>
							<repeat>12</repeat>
							<delta_t>83700</delta_t>
						</pair>
						<pair>
							<repeat>11</repeat>
							<delta_t>378790</delta_t>
						</pair>						
						<pair>
							<repeat>29</repeat>
							<delta_t>5256000</delta_t>
						</pair>
						<pair>
							<repeat>30</repeat>
							<delta_t>15768000</delta_t>
						</pair>
						<pair>
							<repeat>15</repeat>
							<delta_t>5256000</delta_t>
						</pair>
						<pair>
							<repeat>60</repeat>
							<delta_t>2628000</delta_t>
						</pair>
						<pair>
							<repeat>15</repeat>
							<delta_t>5256000</delta_t>
						</pair>
						<pair>
							<repeat>10</repeat>
							<delta_t>15768000</delta_t>
						</pair>
					</timesteps>
				</time_stepping>
			</process>
		</processes>
		<output>
			<type>VTK</type>
			<prefix>OneLayer_IRZ</prefix>
			<timesteps>
				<pair>
					<repeat>1</repeat>
					<each_steps>21</each_steps>
				</pair>
				<pair>
					<repeat>1</repeat>
					<each_steps>35</each_steps>
				</pair>
				<pair>
					<repeat>1</repeat>
					<each_steps>12</each_steps>
				</pair>
				<pair>
					<repeat>1</repeat>
					<each_steps>11</each_steps>
				</pair>
				<pair>
					<repeat>1</repeat>
					<each_steps>29</each_steps>
				</pair>
				<pair>
					<repeat>1</repeat>
					<each_steps>30</each_steps>
				</pair>
				<pair>
					<repeat>1</repeat>
					<each_steps>1</each_steps>
				</pair>
			</timesteps>
            <variables>
                <variable>T</variable>
                <variable>pressure</variable>
				<variable>darcy_velocity</variable>
<!--				<variable>heat_flux</variable>
				<variable>temperature_gradient</variable>-->				
            </variables>
            <suffix>_ts_{:timestep}_t_{:time}</suffix>
        </output>
    </time_loop>
    <parameters>
        <parameter>
            <name>T0</name>
			<type>MeshNode</type>
			<field_name>T_ini</field_name>
		</parameter>
<!--		<parameter>
			<name>T0</name>
			<type>MeshNode</type>			
			<field_name>T_ini</field_name>
		</parameter>-->
		<parameter>
			<name>T_top</name>
			<type>Constant</type>
			<value>323.5</value>		
		</parameter>
		<parameter>
			<name>T_bottom</name>
			<type>Constant</type>
			<value>360</value>
		</parameter>
        <parameter>
            <name>T_inj</name>
            <type>Constant</type>
            <value>303</value>
        </parameter>
        <parameter>
            <name>p_ini</name>
			<type>MeshNode</type>
			<field_name>pressure_ini</field_name>
        </parameter>
		<parameter>
			<name>p_top</name>
			<type>Constant</type>
			<value>9.5e6</value>			
		</parameter>
		<parameter>
			<name>p_bottom</name>
			<type>Constant</type>
			<value>1.88e7</value>
		</parameter>        
        <parameter>
            <name>clay_1_porosity_parameter</name>
            <type>Constant</type>
            <value>0.05</value>
        </parameter>
		<parameter>
			<name>clay_2_porosity_parameter</name>
			<type>Constant</type>
			<value>0.036</value>
		</parameter>
		<parameter>
			<name>sandstone_porosity_parameter_1</name>
			<type>Constant</type>
			<value>0.23</value>
		</parameter>
		<parameter>
			<name>sandstone_porosity_parameter_2</name>
			<type>Constant</type>
			<value>0.19</value>
		</parameter>
		<parameter>
			<name>sandstone_porosity_parameter_3</name>
			<type>Constant</type>
			<value>{{{ :sandstone_porosity_parameter_3 }}}</value>  <!-- 0.2 -->
		</parameter>
		<parameter>
			<name>sandstone_porosity_parameter_4</name>
			<type>Constant</type>
			<value>0.22</value>
		</parameter>
        <parameter>
            <name>kappa_clay</name>
            <type>Constant</type>
            <values>1.97e-17</values>     			<!--isotropic-->
        </parameter>
		<parameter>
			<name>kappa_clay2</name>
			<type>Constant</type>
			<values>9.72e-16</values>
		</parameter>
	
		<parameter>
			<name>kappa_Sandstone_3</name>
			<type>Constant</type>
			<values>{{{ :kappa_sandstone_3 }}}</values>			<!--Permeabilitï¿½t von Sandstein 3   2.39e-13-->
		</parameter>
	

		<parameter>
			<name>outflow</name>
			<type>Constant</type>
			<value>-0.0003782</value>			<!--Hier 0.025mï¿½/s  teilen durch Lï¿½nge-->
		</parameter>
		<parameter>
			<name>inflow</name>
			<type>Constant</type>
			<value>0.0003782</value>			<!--teilen durch Lï¿½nge-->
		</parameter>
				
    </parameters>
    <process_variables>
        <process_variable>
            <name>T</name>
            <components>1</components>
            <order>1</order>
            <initial_condition>T0</initial_condition>

			<boundary_conditions>
				<boundary_condition>
					<geometrical_set>OneLayer</geometrical_set>
					<geometry>top</geometry>
					<type>Dirichlet</type>
					<parameter>T_top</parameter>
				</boundary_condition>
				<boundary_condition>
					<geometrical_set>OneLayer</geometrical_set>
					<geometry>bottom</geometry>
					<type>Dirichlet</type>
					<parameter>T_bottom</parameter>
				</boundary_condition>

				<boundary_condition>
					<mesh>OneLayer_3D_physical_group_Inj</mesh>
					<type>Dirichlet</type>
					<parameter>T_inj</parameter>
				</boundary_condition>	
            </boundary_conditions>
			
        </process_variable>
        <process_variable>
            <name>pressure</name>
            <components>1</components>
            <order>1</order>
            <initial_condition>p_ini</initial_condition>
			
          <boundary_conditions>
                <boundary_condition>
                    <geometrical_set>OneLayer</geometrical_set>
                    <geometry>top</geometry>
                    <type>Dirichlet</type>
                    <parameter>p_top</parameter>
                </boundary_condition>
                <boundary_condition>
                    <geometrical_set>OneLayer</geometrical_set>
                    <geometry>bottom</geometry>
                    <type>Dirichlet</type>
                    <parameter>p_bottom</parameter>
                </boundary_condition>
            </boundary_conditions>

			<source_terms>
				<source_term>
					<mesh>OneLayer_3D_physical_group_Inj</mesh>
					<type>Line</type>
					<parameter>inflow</parameter>
				</source_term>
				<source_term>
					<mesh>OneLayer_3D_physical_group_Pump</mesh>
					<type>Line</type>
					<parameter>outflow</parameter>
				</source_term>									
			</source_terms>
			
        </process_variable>
    </process_variables>
    <nonlinear_solvers>
        <nonlinear_solver>
            <name>basic_picard</name>		<!--auch bei Timeloop einfï¿½gen-->
            <type>Picard</type>
            <max_iter>300</max_iter>
            <linear_solver>general_linear_solver</linear_solver>
        </nonlinear_solver>
    </nonlinear_solvers>
    <linear_solvers>
        <linear_solver>
            <name>general_linear_solver</name>
         <!--   <lis>-i cg -p jacobi -tol 1e-8 -maxiter 5000</lis>-->		<!--integriert-->
            <eigen>
                <!-- <solver_type>PardisoLU</solver_type>
                <precon_type>NONE</precon_type> -->
                <!-- <solver_type>SparseLU</solver_type> -->
                <!-- <precon_type>DIAGONAL</precon_type> -->
                <precon_type>ILUT</precon_type>
                <solver_type>BiCGSTAB</solver_type>
                <max_iteration_step>5000</max_iteration_step>
                <error_tolerance>1e-8</error_tolerance>
            </eigen>
            <!--petsc>
                <prefix>gw</prefix>
                <parameters>-gw_ksp_type cg -gw_pc_type bjacobi -gw_ksp_rtol 1e-8 -gw_ksp_max_it 10000</parameters>
            </petsc-->
        </linear_solver>
    </linear_solvers>
